/**
 * 
 */
package textExcel;

/**
 * @author APCS2
 *
 */
public class TextCell implements Cell {
	private String cellText;
	
	public TextCell(String inputValue){
		cellText = inputValue;
	}
	@Override
	public String abbreviatedCellText() {
		return this.cellText;
	}

	@Override
	public String fullCellText() {
		return this.cellText;
	}

}
