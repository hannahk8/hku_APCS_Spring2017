abstract class Tickets {
	private int number;
	private int price;
	public Tickets(int num){
		this.number = num;
	}
	public int getPrice(){
		return 0;
	}
	public String toString(){
		return "Number: " + this.number + "Price: " + this.price;
	}
}

public class AdvanceTicket extends Tickets{
	public AdvanceTicket(){}
	public int getPrice(){
		if(number >= 10){
			return 30;
		}else{
			return 40;
		}
	}
	public void toString(){
		System.out.println("Number: " + getPrice());
	}
}

public class StudentAdvanceTicket extends AdvanceTicket{
	public StudentAdvanceTicket(){}
	public int getPrice(){
		if(number >= 10){
			return 15;
		}else{
			return 20;
		}
	}
}