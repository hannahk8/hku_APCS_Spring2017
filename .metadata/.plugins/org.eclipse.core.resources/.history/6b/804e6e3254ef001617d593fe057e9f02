public class AdvanceTicket extends Tickets{
	private int price;
	private int numDaysBefore;
	public AdvanceTicket(int n, int daysBefore){
		super(n);
		this.numDaysBefore = daysBefore;
		this.price = getPrice();
	}
	public int getPrice(){
		if(this.numDaysBefore >= 10){
			this.price = 30;
			return this.price;
		}else{
			this.price = 40;
			return this.price;
		}
	}
}
