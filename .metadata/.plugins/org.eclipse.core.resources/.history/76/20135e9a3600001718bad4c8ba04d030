package textExcel;

//Update this file with your own code.

public class SpreadsheetLocation implements Location
{
	private int colNum;
	private int rowNum;
    @Override
    public int getRow()
    {
        // TODO Auto-generated method stub
        return rowNum;
    }

    @Override
    public int getCol()
    {
        // TODO Auto-generated method stub
        return colNum;
    }
    
    public SpreadsheetLocation(String cellName)
    {
        // TODO: Fill this out with your own code
    	int colNum = cellName.charAt(0);
    	int intValue = Integer.parseInt(cellName);
    	int rowNum = intValue - colNum;
    }

}
